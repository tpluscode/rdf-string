{
  "name": "@tpluscode/rdf-string",
  "version": "0.2.24",
  "description": "Simplifies creating of RDF strings using ECMAscript templates",
  "main": "index.js",
  "browser": "index.mjs",
  "module": "index.mjs",
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepack": "npm run build",
    "build": "run-p build:*",
    "build:cjs": "tsc",
    "build:esm": "BABEL_ENV=modules babel src --out-dir . --extensions .ts --out-file-extension .mjs",
    "test": "jest --coverage",
    "docs": "docsify serve docs",
    "release": "changeset publish"
  },
  "files": [
    "*.js",
    "*.mjs",
    "*.d.ts.map",
    "*.d.ts",
    "lib"
  ],
  "dependencies": {
    "@rdf-esm/data-model": "^0.5.3",
    "@rdf-esm/term-map": "^0.5.0",
    "@rdfjs/types": "*",
    "@tpluscode/rdf-ns-builders": "^1",
    "@zazuko/rdf-vocabularies": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@changesets/cli": "^2.16.0",
    "@rdfjs/data-model": "^1.2.0",
    "@rdfjs/dataset": "^1.0.1",
    "@rdfjs/formats-common": "^2.1.0",
    "@rdfjs/namespace": "^1.1.0",
    "@rdfjs/term-set": "^1.1.0",
    "@tpluscode/eslint-config": "^0.3.0",
    "@types/jest": "^27.0.3",
    "@types/luxon": "^1.24.0",
    "@types/rdf-ext": "^1.3.7",
    "@types/rdfjs__dataset": "^1.0.2",
    "@types/rdfjs__formats-common": "^2.0.0",
    "@types/rdfjs__namespace": "^1.1.1",
    "@types/rdfjs__term-map": "^1.0.0",
    "@types/rdfjs__term-set": "^1.0.0",
    "@types/sparqljs": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-plugin-add-import-extension": "^1.5.1",
    "docsify-cli": "^4.4.1",
    "eslint": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^3.1.0",
    "jest": "^27.4.3",
    "lint-staged": "^9.5.0",
    "luxon": "^1.24.1",
    "npm-run-all": "^4.1.5",
    "sparqljs": "^3.0.1",
    "standard": "^16.0.4",
    "string-to-stream": "^3.0.1",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/rdf-string.git"
  },
  "keywords": [
    "rdf",
    "sparql"
  ],
  "author": "Tomasz Pluskiewicz <tomasz.pluskiewicz@zazuko.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/rdf-string/issues"
  },
  "homepage": "https://github.com/tpluscode/rdf-string#readme",
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testRegex": "tests/.+\\.spec\\.ts$",
    "setupFilesAfterEnv": [
      "./tests/jestSetup.ts"
    ],
    "moduleNameMapper": {
      "@rdf-esm/(.*)": "@rdfjs/$1"
    }
  }
}
