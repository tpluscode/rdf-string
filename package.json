{
  "name": "@tpluscode/rdf-string",
  "version": "0.0.5",
  "description": "Simplifies creating of RDF strings using ECMAscript templates",
  "main": "dist/node/index.js",
  "module": "dist/es/index.js",
  "types": "index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "build": "npm run build:node & npm run build:web",
    "build:node": "tsc -p tsconfig.node.json",
    "build:web": "tsc -p tsconfig.es.json",
    "test": "jest --coverage"
  },
  "files": [
    "dist",
    "*.d.ts.map",
    "*.d.ts",
    "lib"
  ],
  "dependencies": {
    "@types/rdf-js": "^2.0.11",
    "@zazuko/rdf-vocabularies": "^2020.2.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@rdfjs/data-model": "^1.1.2",
    "@rdfjs/namespace": "^1.1.0",
    "@tpluscode/eslint-config": "0.0.4",
    "@types/jest": "^25.1.2",
    "@types/rdf-ext": "^1.3.5",
    "@types/rdfjs__namespace": "^1.1.1",
    "@types/sparqljs": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint-plugin-jest": "^23.1.1",
    "husky": "^3.1.0",
    "jest": "^25.1.0",
    "lint-staged": "^9.5.0",
    "sparqljs": "^3.0.1",
    "standard": "^14.3.1",
    "typescript": "^3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zazuko/rdf-string.git"
  },
  "keywords": [
    "rdf",
    "sparql"
  ],
  "author": "Tomasz Pluskiewicz <tomasz.pluskiewicz@zazuko.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zazuko/rdf-string/issues"
  },
  "homepage": "https://github.com/zazuko/rdf-string#readme",
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testRegex": "tests/.+\\.spec\\.ts$",
    "setupFilesAfterEnv": [
      "./tests/jestSetup.ts"
    ]
  }
}
